{
  "name": "portfolio-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src/ api/ e2e/ *.config.{js,ts} scripts/",
    "preview": "vite preview",
    "test": "vitest -c vitest.config.ts",
    "test:ui": "vitest --ui -c vitest.config.ts",
    "test:coverage": "vitest run --coverage -c vitest.config.ts",
    "test:watch": "vitest watch -c vitest.config.ts",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lighthouse http://localhost:4173 --preset=desktop --view",
    "lighthouse:desktop": "lighthouse http://localhost:4173 --preset=mobile --view",
    "prepare": "husky",
    "test:a11y": "vitest run --grep 'Accessibility' -c vitest.config.ts",
    "test:a11y:watch": "vitest --grep 'Accessibility' -c vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:local": "vite --host & npx playwright test"
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "i18next": "^25.6.0",
    "lucide-react": "^0.552.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.66.0",
    "react-i18next": "^16.2.3",
    "react-router-dom": "^7.9.5",
    "react-scroll": "^1.9.3",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-scroll": "^1.8.10",
    "@vercel/node": "^3.0.7",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "@vueuse/core": "^14.0.0",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.2.2",
    "eslint": "^9.36.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest-axe": "^8.0.0",
    "jsdom": "^27.1.0",
    "lighthouse": "^12.0.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vite-plugin-imagemin": "^0.6.1",
    "vitest": "^4.0.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npx eslint --fix",
      "npx prettier --write"
    ],
    "*.{css,md,json}": [
      "npx prettier --write"
    ]
  }
}
